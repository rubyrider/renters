<p id="notice"><%= notice %></p>

<h1>Dashboard Collections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dashboard_collections.each do |dashboard_collection| %>
      <tr>
        <td><%= link_to 'Show', dashboard_collection %></td>
        <td><%= link_to 'Edit', edit_dashboard_collection_path(dashboard_collection) %></td>
        <td><%= link_to 'Destroy', dashboard_collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dashboard Collection', new_dashboard_collection_path %>
