<p id="notice"><%= notice %></p>

<h1>Client Properties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_properties.each do |client_property| %>
      <tr>
        <td><%= link_to 'Show', client_property %></td>
        <td><%= link_to 'Edit', edit_client_property_path(client_property) %></td>
        <td><%= link_to 'Destroy', client_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client Property', new_client_property_path %>
